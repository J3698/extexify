{
  "name": "extexify",
  "description": "Overleaf symbol recognition",
  "version": "1.3",
  "manifest_version": 2,

  "content_scripts": [{
    "js": ["content.js"],
    "css": ["style.css"],
    "matches": ["https://www.overleaf.com/project/*"]
  }],

  "permissions": [
    "*://extexify2.herokuapp.com/*",
    "webRequest"
  ]
}
